{
    "name": "multitenantkit-website",
    "version": "1.0.0",
    "type": "module",
    "private": true,
    "scripts": {
        "dev": "npm run docs:generate && vite",
        "build": "npm run docs:generate && tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
        "deploy": "sst deploy --stage production",
        "deploy:dev": "sst deploy --stage dev",
        "remove": "sst remove",
        "sst:dev": "sst dev",
        "sst:shell": "sst shell",
        "docs:generate": "tsx scripts/generate-docs-routes.ts && npm run fix:safe",
        "fix:safe": "biome check --write ."
    },
    "dependencies": {
        "@mdx-js/react": "^3.1.1",
        "@orama/orama": "^3.1.16",
        "@tailwindcss/typography": "^0.5.19",
        "@tanstack/react-router": "^1.58.3",
        "clsx": "^2.1.1",
        "flexsearch": "^0.8.212",
        "framer-motion": "^11.5.4",
        "fumadocs-core": "^16.0.15",
        "fumadocs-ui": "^16.0.15",
        "github-slugger": "^2.0.0",
        "gray-matter": "^4.0.3",
        "lucide-react": "^0.441.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-intersection-observer": "^9.13.1",
        "react-syntax-highlighter": "^15.5.0",
        "remark-frontmatter": "^5.0.0",
        "remark-mdx-frontmatter": "^5.2.0",
        "shiki": "^3.15.0",
        "tailwind-merge": "^2.5.2"
    },
    "devDependencies": {
        "@mdx-js/rollup": "^3.1.1",
        "@pulumi/cloudflare": "^6.11.0",
        "@tanstack/router-vite-plugin": "^1.58.3",
        "@types/react": "^18.3.5",
        "@types/react-dom": "^18.3.0",
        "@types/react-syntax-highlighter": "^15.5.13",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "@vitejs/plugin-react-swc": "^3.7.0",
        "autoprefixer": "^10.4.20",
        "eslint": "^8.57.1",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.12",
        "postcss": "^8.4.47",
        "prettier": "^3.3.3",
        "rehype-pretty-code": "^0.14.1",
        "rehype-slug": "^6.0.0",
        "remark-gfm": "^4.0.1",
        "sst": "^3.17.23",
        "tailwindcss": "^3.4.11",
        "tsx": "^4.20.6",
        "typescript": "^5.6.2",
        "vite": "^5.4.6"
    }
}
