{
    "name": "@multitenantkit/adapter-persistence-json",
    "version": "0.2.0",
    "type": "module",
    "description": "JSON file-based persistence adapter for MultiTenantKit (development/testing)",
    "keywords": [
        "multitenantkit",
        "saas",
        "multi-tenant",
