{
    "name": "@multitenantkit/composition",
    "version": "0.2.0",
    "type": "module",
    "description": "Dependency injection and composition layer for MultiTenantKit",
    "keywords": [
        "multitenantkit",
        "saas",
        "multi-tenant",
        "multitenancy",
        "composition",
        "dependency-injection",
        "typescript"
    ],
    "author": "",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/multitenantkit/multitenantkit.git",
        "directory": "packages/composition"
    },
    "bugs": {
        "url": "https://github.com/multitenantkit/multitenantkit/issues"
    },
    "homepage": "https://github.com/multitenantkit/multitenantkit#readme",
    "publishConfig": {
        "access": "public"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "README.md"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsc --build",
        "dev": "tsc --build --watch",
        "clean": "rm -rf dist",
        "type-check": "tsc --noEmit",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@multitenantkit/domain": "^0.2.0",
        "@multitenantkit/adapter-persistence-json": "^0.2.0",
        "@multitenantkit/adapter-persistence-postgres": "^0.2.0",
        "@multitenantkit/adapter-system-crypto-uuid": "^0.2.0",
        "@multitenantkit/adapter-system-system-clock": "^0.2.0",
        "@multitenantkit/adapter-metrics-http": "^0.2.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "typescript": "^5.0.0"
    }
}
