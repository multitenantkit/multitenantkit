{
  "name": "@multitenantkit/domain",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./users": {
      "types": "./dist/users/index.d.ts",
      "default": "./dist/users/index.js"
    },
    "./organizations": {
      "types": "./dist/organizations/index.d.ts",
      "default": "./dist/organizations/index.js"
    },
    "./organization-memberships": {
      "types": "./dist/organization-memberships/index.d.ts",
      "default": "./dist/organization-memberships/index.js"
    },
    "./shared": {
      "types": "./dist/shared/index.d.ts",
      "default": "./dist/shared/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --build --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@multitenantkit/domain-contracts": "file:../contracts/domain-contracts",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}